{
  "name": "markocupic/contao-bs-vue-navigation-bundle",
  "type": "contao-module",
  "description": "Contao Bootstraop Vue Navigation Bundle",
  "keywords": [
    "contao",
    "navigation",
    "vue.js",
    "templates"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic/contao-bs-vue-navigation-bundle"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.0",
    "markocupic/contao-component-bootstrap": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ContaoBsVueNavigationBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ContaoBsVueNavigationBundle\\ContaoManager\\Plugin"
  }
}
