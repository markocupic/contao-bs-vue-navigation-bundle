{
    "name": "markocupic/contao-bs-vue-navigation-bundle",
    "type": "contao-module",
    "description": "Contao Bootstrap Vue Navigation Bundle",
    "keywords": [
        "contao",
        "navigation",
        "vue.js",
        "templates"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Cupic",
            "homepage": "https://github.com/markocupic/contao-bs-vue-navigation-bundle"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "markocupic/contao-component-bootstrap": "^5.0",
        "markocupic/contao-twig-assets": "^1.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "contao/test-case": "^4.5",
        "phpunit/phpunit": "^9.3",
        "symfony/http-client": "5.4.*",
        "symfony/phpunit-bridge": "^5.4",
        "contao/easy-coding-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\ContaoBsVueNavigationBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\ContaoBsVueNavigationBundle\\ContaoManager\\Plugin"
    }
}
